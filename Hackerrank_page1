1)Input & Output - Say "Hello, World!
Hello World
#include <stdio.h>
int main() {
    printf("\"Hello, World!\"\n");
    return 0;
}

2)Input & Output - Hello World with tab
#include <stdio.h>
int main() {

    printf("Hello World\tHello World\n");
    return 0;
}

3)Input & Output - Hello World with a new line
#include <stdio.h>
int main() {
    printf("Hello World\nHello World\n");
    return 0;
}

4)Input & Output - Student Details
#include <stdio.h>
#include <math.h>

int main() {

    char name[100];
    int age;
    float cgpa;
    char grade;


    scanf("%s", name);
    scanf("%d", &age);
    scanf("%f", &cgpa);
    scanf(" %c", &grade);


    cgpa = floor(cgpa * 100) / 100;


    printf("Name: %s\n", name);
    printf("Age: %d\n", age);
    printf("CGPA: %.2f\n", cgpa);
    printf("Grade: %c\n", grade);

    return 0;

}


5)Input & Output - ASCII Values - I
#include <stdio.h>
int main() {
    char character;
    
   r
    scanf("%c", &character);
    

    printf("%d\n", (int)character);
    
    return 0;
}


6)Input & Output - ASCII Values - II
#include <stdio.h>

int main() {
    int ascii_value;
    

    scanf("%d", &ascii_value);
    
   
    printf("%c\n", (char)ascii_value);
    
    return 0;
}


7)Input & Output - Round Off
#include <stdio.h>
#include <math.h>

int main() {
    float value;
    
    scanf("%f", &value);
 
    printf("%d\n", (int)value);
    
  
    printf("%d\n", (int)ceil(value));

    printf("%d\n", (int)floor(value));
    
    return 0;
}


8)Operators - Fencing the Ground
#include <stdio.h>

int main() {
    int length, breadth;
    
   
    scanf("%d %d", &length, &breadth);
    
  
    int perimeter = 2 * (length + breadth);
   
    int area = length * breadth;
    
    
    printf("The required length is %d m\n", perimeter);
    printf("The required area of carpet is %d sqm\n", area);
    
    return 0;
}


9)Operators - The Newspaper Agency
#include <stdio.h>

int main() {
    int a, b, c;
    
    
    scanf("%d %d %d", &a, &b, &c);
        int revenue = a * b;
    int total_cost = (a * c) + 100;
    
   
    int profit = revenue - total_cost;
    
  
    printf("%d\n", profit);
    
    return 0;
}


10)Harry potter
int main() {
    int num;
    scanf(%d",&num);
    int first=num/1000;
    int last=num % 10;
    int sum=first+last;
    printf("%d",sum);


    return 0;
}
